#include<iostream>
#include<cmath>
using namespace std;
int main()
{
	//Set the username and password. Let the user type the username and password and print if it is the same or not.
	
	string username, x="TaylorSwift";
	string password, y="TTPD31songs";
	
	cout<<"Enter your username: ";
	cin>>username;
	
	cout<<"Enter your password: ";
	cin>>password;
	
	cout<<"\n";

	if(username == x && password == y){
		cout<<"The username and password you entered are the same.";
	}
	else{
		cout<<"The username and password you entered are not the same.";
	}

	return 0;	
	
}
